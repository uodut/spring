<?xml version="1.0" encoding="UTF-8" ?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
 http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">   <bean id="boss" class="com.nubia.spring.annotationTest.Boss">
        <property name="car" ref="car"/>
        <property name="office" ref="office" />
    </bean>
    <bean id="office" class="com.nubia.spring.annotationTest.Office">
        <property name="officeNo" value="002"/>
    </bean>
    <bean id="car" class="com.nubia.spring.annotationTest.Car" scope="singleton">
        <property name="brand" value="红旗 CA72"/>
        <property name="price" value="2000"/>
    </bean>
</beans>
 
 
